      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5
      },
      "enableLiveMetrics": true
    },
    "logLevel": {
      "default": "Information",
      // Bump your functionâ€™s logger to Debug when needed
      "Function.qc_http_function": "Debug",
      // Useful categories:
      "azure.functions.worker": "Information",
      "host.results": "Information"
    }
  }
}
